  					<row>
  						<entry align="center">Parameter</entry>
  						<entry align="center">Description</entry>
  						
  					</row>
  				</thead>
  				<tbody>
  					<row>
  						<entry><codeph>group_name</codeph></entry>
  						<entry>Name of the group to add the rule to.</entry>
  						
  					</row>
  					
  				</tbody>
  			</tgroup>
  		</table>
  	</section>
