<reference id="euca_bundle_image">
	<title>euca_bundle_image</title>
	<shortdesc>Bundles an image for use with Eucalyptus or Amazon EC2.</shortdesc>
  <refbody>
	<section>
  	<title>Syntax</title>
  	<codeblock>euca-bundle-image  -i, --image &lt;image_path> [-a, --access-key &lt;access_key> ][-s,
     --secret-key &lt;secret_key> ][--config &lt;config_path>
     ][--debug][--debugger][-h, --help][--region &lt;region_name>
     ][-U, --url &lt;url> ][--version][--euca-auth][-u, --user &lt;user>
     ][-c, --cert &lt;cert_path> ][-k, --privatekey
     &lt;private_key_path> ][-p, --prefix &lt;prefix> ][--kernel
     &lt;kernel_id> ][--ramdisk &lt;ramdisk_id> ][--product-codes
     &lt;product_codes> ][-b, --block-device-mapping
     &lt;block_device_mapping> ][-d, --destination &lt;destination>
     ][--ec2cert &lt;ec2cert_path> ][-r, --arch &lt;target_arch>
     ][--batch]</codeblock>
  </section>
