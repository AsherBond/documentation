<reference id="euca-confirm-product-instance">
	<title>euca-confirm-product-instance</title>
	<shortdesc>Confirm if instance is running with product code attached.</shortdesc>
  <refbody>
   <section>
   	<title>Syntax</title>
   	<codeblock>euca-confirm-product-instance  -i, --instance &lt;instance_id> [-a, --access-key
     &lt;access_key> ][-s, --secret-key &lt;secret_key>
     ][--config &lt;config_path>
     ][--debug][--debugger][-h, --help][--region
     &lt;region_name> ][-U, --url &lt;url> ][--version
     ][--euca-auth] product_code</codeblock>
   </section>
