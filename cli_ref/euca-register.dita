<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="euca-register">
	<title>euca-register</title>
	<shortdesc>Registers a manifest for use with the cloud.</shortdesc>
  <refbody>
    <section>
    	<title>Syntax</title>
    	<codeblock>euca-register  [-A, --access-key &lt;access_key> ][-A, --secret-key &lt;secret_key>
     ][--config &lt;config_path> ][--debug][--debugger][-h,
     --help][--region &lt;region_name> ][-U, --url &lt;url> ][--version
     ][--euca-auth][-b, --block-device-mapping &lt;block_device_mapping>
     ][-d, --description &lt;description> ][-n, --name &lt;name> ][-a,
     --architecture &lt;architecture> ][--kernel &lt;kernel> ][--ramdisk
     &lt;ramdisk> ][--root-device-name &lt;root_device_name> ][-s,
     --snapshot &lt;snapshot> ] [ image_location ]</codeblock>
    </section>
  	<section>
  		<title>Required Parameters</title>
  		<table>
  			<tgroup cols="2">
  				<colspec colwidth="20*"/>
  				<colspec colwidth="80*"/>
  				
  				<thead>
  					<row>
  						<entry align="center">Parameter</entry>
  						<entry align="center">Description</entry>
  						
  					</row>
  				</thead>
  				<tbody>
  					<row>
  						<entry><codeph>-n,--name</codeph></entry>
  						<entry>Name of the image</entry>
  					</row>
  				</tbody>
  			</tgroup>
  		</table>
  	</section>
  	<section>
  		<title>Optional Parameters</title>
  		<table>
  			<tgroup cols="2">
  				<colspec colwidth="20*"/>
  				<colspec colwidth="80*"/>
  				<thead>
  					<row>
  						<entry align="center">Option</entry>
  						<entry align="center">Description</entry>
  					</row>
  					
  				</thead>
  				<tbody>
  					<row conref="../shared/shared_params.dita#shared_params/access_key_id">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/secret_key">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/config">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/debug">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/debugger">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/help">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/region">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/url">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/version">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/euca_auth">
  						<entry></entry>
  					</row>
  					<row>
  						<entry><codeph>-b,--block-device-mapping</codeph></entry>
  						<entry>Block device mapping for the instance(s).
  							Option may be used multiple times.
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>-d,--description</codeph></entry>
  						<entry>Description of the image</entry>
  					</row>
  					<row>
  						<entry><codeph>-a,--architecture</codeph></entry>
  						<entry>The architecture of the image
  							<p>Valid Values: <codeph>i386</codeph> | <codeph>x86_64</codeph></p></entry>
  					</row>
  					<row>
  						<entry><codeph>--kernel</codeph></entry>
  						<entry>The ID of the kernel associated with the
  							image
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>--ramdisk</codeph></entry>
  						<entry> The ID of the ramdisk associated with the
  							image
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>--root-device-name</codeph></entry>
  						<entry>The root device name (for example, /dev/sda1, or
  							xvda)
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>-s,--snapshot</codeph></entry>
  						<entry>The snapshot ID to use as the root device</entry>
  					</row>
  					<row>
  						<entry><codeph>image_location</codeph></entry>
  						<entry>Path to the uploaded image (bucket/manifest).
  							Required if you are registering an S3-based image
  						</entry>
  					</row>
  				</tbody>
  			</tgroup>
  		</table>
  	</section>
