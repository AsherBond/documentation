    <title class="- topic/title " xtrf="/Users/scottb/Documents/git/documentation/admin_guide/group_rename.dita" xtrc="title:1">Modify a Group</title>
    <shortdesc class="- topic/shortdesc " xtrc="shortdesc:1" xtrf="/Users/scottb/Documents/git/documentation/admin_guide/group_rename.dita"/>
	<prolog class="- topic/prolog " xtrc="prolog:1" xtrf="/Users/scottb/Documents/git/documentation/admin_guide/group_rename.dita">
