    			<row class="- topic/row " xtrf="/Users/scottb/Documents/git/documentation/admin_guide/ldap_service.dita" xtrc="row:4">
    				<entry colname="col1" class="- topic/entry " xtrf="/Users/scottb/Documents/git/documentation/admin_guide/ldap_service.dita" xtrc="entry:7">auth-principal</entry>
