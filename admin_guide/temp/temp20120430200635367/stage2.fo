 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt"><fo:inline font-size="10pt" font-family="monospace">NOTREADY</fo:inline></fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">No</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">No</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">Service is failing to operate correctly</fo:block></fo:table-cell>
 					</fo:table-row>
