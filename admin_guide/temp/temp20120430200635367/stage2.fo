 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">VMware</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">Windows</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">See <fo:inline/><fo:basic-link color="blue" font-style="italic" internal-destination="unique_159">Create Windows Image (VMware)</fo:basic-link></fo:block></fo:table-cell>
