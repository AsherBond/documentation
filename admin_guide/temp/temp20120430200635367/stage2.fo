            	<fo:block space-before="3pt" space-after="3pt">
            		<fo:block font-size="10pt" space-before="0.6em" space-after="0.6em" white-space-treatment="preserve" white-space-collapse="false" linefeed-treatment="preserve" wrap-option="wrap" background-color="#f0f0f0" font-family="Monospaced" line-height="106%" border-top-style="dashed" border-top-width="1pt" border-top-color="#6A737B" border-bottom-style="dashed" border-bottom-width="1pt" border-bottom-color="#6A737B" border-left-style="dashed" border-left-width="1pt" border-left-color="#6A737B" border-right-style="dashed" border-right-width="1pt" border-right-color="#6A737B" keep-with-previous.within-page="always" start-indent="6pt + from-parent(start-indent)" end-indent="6pt + from-parent(end-indent)" padding="6pt">service eucalyptus-cc restart</fo:block>
