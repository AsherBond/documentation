 						<fo:table-cell background-color="#03405F" text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt" font-weight="bold" margin="3pt 3pt 3pt 3pt" color="white" text-align="center" font-family="Helvetica">Operational</fo:block></fo:table-cell>
 						<fo:table-cell background-color="#03405F" text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt" font-weight="bold" margin="3pt 3pt 3pt 3pt" color="white" text-align="center" font-family="Helvetica">In use by system</fo:block></fo:table-cell>
 						<fo:table-cell background-color="#03405F" text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt" font-weight="bold" margin="3pt 3pt 3pt 3pt" color="white" text-align="center" font-family="Helvetica">Description</fo:block></fo:table-cell>
 					</fo:table-row>
 				</fo:table-header>
 				<fo:table-body>
 					<fo:table-row>
 						<fo:table-cell text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt"><fo:inline font-size="10pt" font-family="monospace">ENABLED</fo:inline></fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">Yes</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">Yes</fo:block></fo:table-cell>
 						<fo:table-cell text-align="from-table-column()" border-top-style="solid" border-top-width="1pt" border-top-color="black"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt">Service is operating correctly and is selected for processing requests</fo:block></fo:table-cell>
 					</fo:table-row>
 					<fo:table-row>
 						<fo:table-cell text-align="from-table-column()"><fo:block space-before="3pt" space-before.conditionality="retain" space-after="3pt" space-after.conditionality="retain" start-indent="3pt" end-indent="3pt"><fo:inline font-size="10pt" font-family="monospace">DISABLED</fo:inline></fo:block></fo:table-cell>
