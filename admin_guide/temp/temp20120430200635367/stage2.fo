					<fo:list-item space-after="1.5pt" space-before="1.5pt"><fo:list-item-label keep-together.within-line="always" keep-with-next.within-line="always" end-indent="label-end()"><fo:block text-align="start"><fo:inline/>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>host where service is registered</fo:block></fo:list-item-body></fo:list-item> 
					<fo:list-item space-after="1.5pt" space-before="1.5pt"><fo:list-item-label keep-together.within-line="always" keep-with-next.within-line="always" end-indent="label-end()"><fo:block text-align="start"><fo:inline/>•</fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>partition where service is registered</fo:block></fo:list-item-body></fo:list-item>
