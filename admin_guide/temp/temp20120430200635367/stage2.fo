 	<fo:list-block space-before="0.6em" space-after="0.6em" provisional-distance-between-starts="5mm" provisional-label-separation="1mm">
 		<fo:list-item space-after="3pt" space-before="3pt"><fo:list-item-label keep-together.within-line="always" keep-with-next.within-line="always" end-indent="label-end()"><fo:block text-align="start" font-weight="bold"><fo:inline/>1. </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
 			<fo:block>Run an instance of the image.</fo:block>
 		</fo:block></fo:list-item-body></fo:list-item>
 		
 		<fo:list-item space-after="3pt" space-before="3pt"><fo:list-item-label keep-together.within-line="always" keep-with-next.within-line="always" end-indent="label-end()"><fo:block text-align="start" font-weight="bold"><fo:inline/>2. </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
