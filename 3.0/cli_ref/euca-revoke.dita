<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="euca-revoke">
	<title>euca-revoke</title>
	<shortdesc>Revokes a rule for a security group.</shortdesc>
  <refbody>
    <section>
    	<title>Syntax</title>
    	<codeblock>euca-revoke  [-A, --access-key &lt;access_key> ][-A, --secret-key &lt;secret_key>
     ][--config &lt;config_path> ][--debug][--debugger][-h,
     --help][--region &lt;region_name> ][-U, --url &lt;url> ][--version
     ][--euca-auth][-P, --protocol &lt;protocol> ][-p, --port-range
     &lt;port_range> ][-t, --icmp-type-code &lt;icmp_type_code> ][-o,
     --source-group &lt;source_group> ][-u, --source-group-user
     &lt;source_group_user> ][-s, --source-subnet &lt;source_subnet> ]
     group_name</codeblock>
    </section>
  	<section>
  		<title>Required Parameters</title>
  		<table>
  			<tgroup cols="2">
  				<colspec colwidth="20*"/>
  				<colspec colwidth="80*"/>
  				
  				<thead>
  					<row>
  						<entry align="center">Parameter</entry>
  						<entry align="center">Description</entry>
  						
  					</row>
  				</thead>
  				<tbody>
  					<row>
  						<entry><codeph>group_name</codeph></entry>
  						<entry>Name of the group to remove the rule from</entry>
  					</row>
  				</tbody>
  			</tgroup>
  		</table>
  	</section>
  	<section>
  		<title>Optional Parameters</title>
  		<table>
  			<tgroup cols="2">
  				<colspec colwidth="20*"/>
  				<colspec colwidth="80*"/>
  				<thead>
  					<row>
  						<entry align="center">Option</entry>
  						<entry align="center">Description</entry>
  					</row>
  					
  				</thead>
  				<tbody>
  					<row conref="../shared/shared_params.dita#shared_params/access_key_id">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/secret_key">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/config">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/debug">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/debugger">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/help">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/region">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/url">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/version">
  						<entry></entry>
  					</row>
  					<row conref="../shared/shared_params.dita#shared_params/euca_auth">
  						<entry></entry>
  					</row>
  					<row>
  						<entry><codeph>-P,--protocol</codeph></entry>
  						<entry>The protocol used in the rule.
  							<p>Valid Values: <codeph>tcp</codeph> | <codeph>udp</codeph> | <codeph>icmp</codeph> |
  								<codeph>6</codeph> | <codeph>17</codeph> | <codeph>1</codeph></p></entry>
  					</row>
  					<row>
  						<entry><codeph>-p,--port-range</codeph></entry>
  						<entry>Range of ports for the rule, specified as
  							<codeph>from-to</codeph>
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>-t,--icmp-type-code</codeph></entry>
  						<entry>ICMP type and code specified as <codeph>type:code</codeph></entry>
  					</row>
  					<row>
  						<entry><codeph>-o,--source-group</codeph></entry>
  						<entry>Group from which traffic is authorized
  							by the rule
  						</entry>
  					</row>
  					<row>
  						<entry><codeph>-u,--source-group-user</codeph></entry>
  						<entry>User ID for the source group</entry>
  					</row>
  					<row>
  						<entry><codeph>-s,--source-subnet</codeph></entry>
  						<entry>The source subnet for the rule.
  							<p>Default: <codeph>0.0.0.0/0</codeph></p>
  						</entry>
  					</row>
  				</tbody>
  			</tgroup>
  		</table>
  	</section>
